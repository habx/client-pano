{
  "name": "client-template",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "build:schema": "habx-graphql build-schema",
    "build:types": "habx-graphql build-types --passthroughCustomScalars",
    "build:graphql": "npm run build:schema && npm run build:types",
    "clean:types": "habx-graphql clean-types",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "build-ci": "DISABLE_ESLINT_PLUGIN=true tsc && vite build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/habx/client-template.git"
  },
  "author": "HABX Front End team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/habx/client-template/issues"
  },
  "dependencies": {
    "@apollo/client": "3.7.0",
    "@habx/apollo-multi-endpoint-link": "2.9.0",
    "@habx/lib-client-acl": "13.6.0",
    "@habx/lib-client-apollo-utilities": "3.9.0",
    "@habx/lib-client-intl": "^3.0.0",
    "@habx/lib-client-orbital": "^10.8.0",
    "@habx/lib-client-reporting": "5.2.0",
    "@habx/lib-form-helper": "8.2.0",
    "@habx/ui-core": "6.21.0",
    "events": "3.3.0",
    "final-form": "4.20.7",
    "final-form-arrays": "3.0.2",
    "graphql": "15.8.0",
    "lodash": "4.17.21",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-final-form": "6.5.9",
    "react-router-dom": "^6.6.1",
    "styled-components": "5.3.6"
  },
  "devDependencies": {
    "@habx/config-ci-front": "3.9.0",
    "@habx/eslint-config-client": "6.10.0",
    "@habx/eslint-config-graphql": "1.9.0",
    "@habx/graphql-scripts": "6.0.0",
    "@types/lodash": "4.14.191",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.26",
    "@vitejs/plugin-legacy": "3.0.1",
    "@vitejs/plugin-react": "^3.0.0",
    "husky": "4.3.8",
    "lint-staged": "13.1.0",
    "terser": "5.16.1",
    "typescript": "4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-checker": "^0.4.4"
  }
}
